# Environment Variables Template
# Copy this file to .env and fill in your Feishu credentials

# Feishu App Credentials
FEISHU_APP_ID=your_app_id_here
FEISHU_APP_SECRET=your_app_secret_here

# Feishu Table Configuration
FEISHU_TABLE_APP_TOKEN=your_table_app_token_here
FEISHU_TABLE_ID=your_table_id_here

# Optional: Custom API endpoint (leave empty for default)
# FEISHU_API_ENDPOINT=https://open.feishu.cn
